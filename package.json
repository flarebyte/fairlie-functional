{
  "name": "fairlie-functional",
  "description": "A lightweight functional library in typescript that helps dealing with failures the rail way",
  "version": "0.1.0",
  "author": {
    "name": "Olivier Huin",
    "url": "https://github.com/olih"
  },
  "keywords": [
    "ROP",
    "Railway oriented programming"
  ],
  "license": "MIT",
  "homepage": "https://github.com/flarebyte/fairlie-functional",
  "repository": {
    "type": "git",
    "url": "https://github.com/flarebyte/fairlie-functional.git"
  },
  "bugs": "https://github.com/flarebyte/fairlie-functional/issues",
  "type": "module",
  "types": "./dist/src/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/src/index.mjs",
      "default": "./dist/src/index.mjs",
      "types": "./dist/src"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "main": "./dist/src/index.mjs",
  "files": [
    "dist/src",
    "src"
  ],
  "bin": {},
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --outDir dist",
    "test": "node --test --loader ts-node/esm test/*.test.ts",
    "spec": "node --loader ts-node/esm .baldrick-zest.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.2.5",
    "baldrick-zest-engine": "^0.9.0",
    "baldrick-zest-mess": "^0.17.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "xo": "^0.55.0"
  },
  "peerDependencies": {}
}
